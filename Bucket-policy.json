{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowAccountBAccess",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::23xxxxxxxxx2:root"
            },
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": ""arn:aws:s3:::mybucketforss3/*"
        },
        {
            "Sid": "AllowAccountBListBucket",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::23xxxxxxxxxx5:root"
            },
            "Action": "s3:ListBucket",
            "Resource": ""arn:aws:s3:::mybucketforss3/*"
        }
    ]
}
